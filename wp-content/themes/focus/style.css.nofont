	}

	#content {
		width: 390px;
	}

	.content-sidebar #content,
	.sidebar-content #content {
		width: 600px;
	}

	.full-width-content #content {
		width: 850px;
	}

	.sidebar {
		width: 240px;
	}

	#sidebar-alt {
		width: 120px;
	}

	.enews #subbox {
		background: #fff;
		padding: 15px 0 13px 10px;
	}

